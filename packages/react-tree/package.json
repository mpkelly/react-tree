    "tree",
    "drag",
    "drop",
    "sorted"
  ],
  "author": "mkelly <kikemelly@gmail.com>",
  "homepage": "https://github.com/mpkelly/react-tree",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mpkelly/react-tree.git"
  },
  "bugs": {
    "url": "https://github.com/mpkelly/react-tree/issues"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/Index.d.ts",
  "devDependencies": {
    "@4tw/cypress-drag-drop": "github:4teamwork/cypress-drag-drop",
    "@babel/core": "^7.9.6",
    "@babel/plugin-transform-async-to-generator": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/runtime": "^7.9.6",
    "@types/jest": "^25.2.3",
    "@types/mocha": "^7.0.2",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "*",
    "jest": "^26.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "terser-webpack-plugin": "^2.3.6",
    "ts-jest": "^26.1.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-glob": "^2.0.2"
  },
  "scripts": {
    "test-jest": "jest",
    "test-testcafe": "webpack  --config ./webpack.test.config.js --mode=production && testcafe chrome ./src/Tree.testcafe.ts",
    "test": "npm run test-jest && npm run test-testcafe",
    "watch": "webpack -w --mode=development",
    "typings": "tsc -d --emitDeclarationOnly --allowJs false --esModuleInterop true --skipLibCheck true --jsx react --outDir ./dist src/Index.ts",
    "dist": "webpack --mode=production && npm run typings",
    "publish": "npm run dist && npm run test && npx lerna publish"
  },
  "dependencies": {
    "testcafe": "^1.8.6"
  }
